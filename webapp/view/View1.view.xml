<mvc:View controllerName="project122.controller.View1" height="100%" width="100%" displayBlock="true" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
    <Page id="page" title="{i18n>title}" class="sapUiContentPadding">
        <content>
            <Toolbar>
                <ToolbarSpacer />
                <SearchField liveChange="onSearch" width="30%" />
            </Toolbar>

            <ScrollContainer height="100%" width="100%" vertical="true" focusable="true">
                <Table id="idProducts" width="100rem" items="{
            path : 'oModel1>/results' }" sticky="ColumnHeaders" alternateRowColors="true">
                    <columns>
                        <Column mergeDuplicates="true" width="10rem">
                            <Text text="ShipName" />
                        </Column>

                        <Column width="10rem">
                            <Text text="ShipAddress	" />
                        </Column>
                       <Column width="9rem">
                            <Text text="ShipCity" />
                        </Column> 
                           
                            <Column width="9rem">
                            <Text text="ShipRegion" />
                        </Column> 

                        <Column width="9rem">
                            <Text text="ShipPostalCode" />
                        </Column>

                        <Column width="7rem">
                            <Text text="ShipCountry" />
                        </Column>

                        <Column width="7rem">
                            <Text text="CustomerID" />
                        </Column>

                        <Column width="9rem">
                            <Text text="CustomerName" />
                        </Column>

                        <Column width="7rem">
                            <Text text="Address" />
                        </Column>

                        <Column width="7rem">
                            <Text text="City" />
                        </Column>

                        <Column width="7rem">
                            <Text text="Region" />
                        </Column>

                        <Column width="7rem">
                            <Text text="PostalCode" />
                        </Column>

                        <Column width="7rem">
                            <Text text="Country" />
                        </Column>
                        <Column width="7rem">
                            <Text text="Salesperson" />
                        </Column>

                        <Column width="7rem">
                            <Text text="OrderID" />
                        </Column>
                        <Column width="7rem">
                            <Text text="OrderDate" />
                        </Column>
                        <Column width="9rem">
                            <Text text="RequiredDate" />
                        </Column>
                        <Column width="7rem">
                            <Text text="ShippedDate" />
                        </Column>
                        <Column width="7rem">
                            <Text text="ShipperName" />
                        </Column>
                        <Column width="7rem">
                            <Text text="ProductID" />
                        </Column>
                        <Column width="7rem">
                            <Text text="ProductName" />
                        </Column>
                        <Column width="7rem">
                            <Text text="UnitPrice" />
                        </Column>
                        <Column width="7rem">
                            <Text text="Quantity" />
                        </Column>
                        <Column width="7rem">
                            <Text text="Discount" />
                        </Column>
                        <Column width="9rem">
                            <Text text="ExtendedPrice" />
                        </Column>
                        <Column width="7rem">
                            <Text text="Freight" />
                        </Column>
                    </columns>
                    <items>
                        <ColumnListItem type="Active" id="colss" press="onPress">
                            <cells>
                                <Text text="{oModel1>ShipName}" />
                                <Text text="{oModel1>ShipAddress}" />
                                <Text text="{oModel1>ShipCity}" />
                                <Text text="{oModel1>ShipRegion}" />
                                <Text text="{oModel1>ShipPostalCode}" />
                                <Text text="{oModel1>ShipCountry}" />
                                <Text text="{oModel1>CustomerID}" />
                                <Text text="{oModel1>CustomerName}" />
                                <Text text="{oModel1>Address}" />
                                <Text text="{oModel1>City}" />
                                <Text text="{oModel1>Region}" />
                                <Text text="{oModel1>PostalCode}" />
                                <Text text="{oModel1>Country}" />
                                <Text text="{oModel1>Salesperson}" />
                                <Text text="{oModel1>OrderID}" />
                                <Text text="{oModel1>OrderDate}" />
                                <Text text="{oModel1>RequiredDate}" />
                                <Text text="{oModel1>ShippedDate}" />
                                <Text text="{oModel1>ShipperName}" />
                                <Text text="{oModel1>ProductID}" />
                                <Text text="{oModel1>ProductName}" />
                                <Text text="{oModel1>UnitPrice}" />
                                <Text text="{oModel1>Quantity}" />
                                <Text text="{oModel1>Discount}" />
                                <Text text="{oModel1>ExtendedPrice}" />
                                <Text text="{oModel1>Freight}" />
                            </cells>
                        </ColumnListItem>
                    </items>
                </Table>
            </ScrollContainer>
        </content>
    </Page>
</mvc:View>
